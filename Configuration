# CREATE WAREHOUSE

CREATE WAREHOUSE COMPUTE_WAREHOUSE 
WITH 
WAREHOUSE_SIZE = XSMALL
MAX_CLUSTER_COUNT = 3
AUTO_SUSPEND = 300
AUTO_RESUME = TRUE
COMMENT = "To jest nasza druga jednostka"

# UPDATE WAREHOUSE
CREATE OR REPLACE WAREHOUSE COMPUTE_WAREHOUSE
WITH 
WAREHOUSE_SIZE = XSMALL
MAX_CLUSTER_COUNT = 3
AUTO_SUSPEND = 300
AUTO_RESUME = TRUE
INITIALLY_SUSPENDED = TRUE
COMMENT = "This is our second unit"

# DROP WAREHOUSE

DROP WAREHOUSE WAREHOUSE_SECOUND 
